{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@api/*": [
        "src/pages/api/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@contexts": [
        "src/contexts"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@public/*": [
        "public/*"
      ],
      "@public": [
        "public"
      ],
      "@styled-components/*": [
        "src/styled-components/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@types": [
        "types/index.ts"
      ]
    },
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/logtime/index.jsx",
    "next.config.js",
    "src/pages/api/auth/[...nextauth].js",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
